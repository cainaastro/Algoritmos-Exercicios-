algoritmo "OrdenaVetor"
var
   vet: vetor[1..10] de Inteiro
   aux, j, i : Inteiro
inicio
      para i de 1 ate 10 faca
           Escreva("Digite um valor: ")
           Leia(vet[i])
      Fimpara

      Para i de 1 ate 9 faca
           para j de i + 1 ate 10 faca
                se (vet[i] > vet[j]) entao
                   aux <- vet[i]
                   vet[i] <- vet[j]
                   vet[j] <- aux
                Fimse
           fimpara
      fimpara
      
      para i de 1 ate 10 faca
           Escreva("{", vet[i], "}")
      fimpara
fimalgoritmo
